@{
    ViewData["Title"] = "Home Page";
}

<div class="prodscats">
    <a href="/">Products |</a>
    <a href="/categories"> Categories</a>
</div>

<div class="main-content">
    <div class="prod_partial col-6">
        <partial name="CategoryFormPartial"/>
    </div>
    <div class="existing-prod col-6">
        <center>
            <h2>Existing Products</h2>
            @foreach(var c in ViewBag.Categories)
            {
                <li><a href="/new/@c.CategoryId">@c.Name</a></li>
            }
        </center>
    </div>
</div>